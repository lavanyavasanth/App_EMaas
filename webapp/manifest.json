{
    "_version": "1.32.0",
    "sap.app": {
        "id": "bts.testapp.testproject1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "bts.testapp.testproject1.view.TestView",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.93.0",
            "libs": {
                "sap.ui.core": {},
                "sap.tnt": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "bts.testapp.testproject1.i18n.i18n"
                }
            },
            "empdata": {

                "type": "sap.ui.model.json.JSONModel",

                "uri": "model/empdata.json"

            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "bts.testapp.testproject1.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false,



                "sample":{
                    "stretch":true,
                    "files":[
                        "TestView.view.xml",
                        "TestView.controller.js",
                        "Change.fragment.xml",
					    "Display.fragment.xml",
                        "manifest.json"
                    ]
                }

                
            },
            "routes": [
                {
                    "name": "RouteTestView",
                    "pattern": "RouteTestView",
                    "target": [
                        "TargetTestView"
                    ]
                },
                {
                    "name": "home",
                    "pattern": "",
                    "target": [
                        "TargetHome"
                    ]
                },
                {
                    "name": "login",
                    "pattern": "login",
                    "target": [
                        "TargetLogin"
                    ]
                },
                {
                    "name": "detail",
                    "pattern": "detail",
                    "target": [
                        "TargetDetail"
                    ]
                },
                {
                    "name": "profilePage",
                    "pattern": "profilePage",
                    "target": [
                        "TargetprofilePage"
                    ]
                }, 
                {
                    "name": "HRadmin",
                    "pattern": "HRadmin",
                    "target": [
                        "TargetHRadmin"
                    ]
                }
            ],
            "targets": {
                "TargetTestView": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "TestView",
                    "viewName": "TestView"
                },
                "TargetHome": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "home",
                    "viewName": "home"
                },
                "TargetLogin": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "login",
                    "viewName": "login"
                },
                "TargetDetail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "detail",
                    "viewName": "detail"
                },
                "TargetprofilePage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "profilePage",
                    "viewName": "profilePage"
                },
                "TargetHRadmin": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "HRadmin",
                    "viewName": "HRadmin"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "bts-testapp-testproject1"
    }
}